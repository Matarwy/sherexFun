<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="ic_launcher_background">#FFFFFF</color>
    <string name="app_name">SherexFun</string>
    <string name="custom_url_scheme">fun.sherex.mobile</string>
    <string name="package_name">fun.sherex.mobile</string>
    <string name="title_activity_main">SherexFun</string>
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="AppTheme.NoActionBar" parent="Theme.AppCompat.DayNight.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:background">@null</item>
    </style>
    <style name="AppTheme.NoActionBarLaunch" parent="Theme.SplashScreen">
        
        <item name="android:windowSplashScreenBackground">@drawable/splash</item>

        
        <item name="android:statusBarColor">#f58c06</item>
        <item name="android:navigationBarColor">#f58c06</item>

        
        <item name="android:windowLightStatusBar">false</item>
        <item name="android:windowLightNavigationBar">false</item>
    </style>
</resources>